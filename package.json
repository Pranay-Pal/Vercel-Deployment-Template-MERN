{
  "name": "vercel-mern",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Template to deploy express js to vercel.",
  "author": "Pranay Pal",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "set +v ; npm run build ; vercel --prod ; echo [...Deployment Successful...] || @echo off && npm run build && vercel --prod &&  echo [...Deployment Successful...] || stty -echo && npm run build && vercel --prod &&  echo [...Deployment Successful...]"
  },
  "url":"https://github.com/Pranay-Pal/Vercel-Deployment-Template-MERN.git",
  "bugs": {
    "url": "https://github.com/Pranay-Pal/Vercel-Deployment-Template-MERN/issues"
  },
  "keywords": [
    "web development",
    "MERN stack",
    "template",
    "Express.js",
    "Vercel",
    "deployment",
    "server",
    "MongoDB",
    "frontend",
    "development tools"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "express": "^4.19.2",
    "mongoose": "^8.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "vite": "^5.2.8"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "vercel": "^32.3.0"
  }
}
